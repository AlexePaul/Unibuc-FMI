{
  "name": "calculator",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest"
    },
    "testMatch": [
      "<rootDir>/components/testing/*.test.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 10,
        "functions": 10,
        "lines": 10,
        "statements": -30
      }
    }
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.3.29",
    "babel-jest": "^29.7.0",
    "expo": "^50.0.14",
    "expo-status-bar": "~1.11.1",
    "jest": "^29.7.0",
    "react": "18.2.0",
    "react-native": "0.73.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.24.3",
    "@testing-library/react-native": "^12.4.5",
    "react-test-renderer": "^18.2.0"
  },
  "private": true
}
